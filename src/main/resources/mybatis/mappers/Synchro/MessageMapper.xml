<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.system.mapper.Synchro.MessageMapper">
    <insert id="InsetintoMessage" parameterType="java.util.Map">
        insert into ctp_user_history_message(ID
        ,USER_ID
        ,REFERENCE_ID
        ,SENDER_ID
        ,MESSAGE_CATEGORY
        ,MESSAGE_TYPE
        ,MESSAGE_CONTENT
        ,CREATION_DATE
        ,IS_READ
        ,OPEN_TYPE
        ,IMPORTANT_LEVEL
        ,LINK_TYPE
        ,IS_AT
        ,IS_TRACK
        ,IS_REPLY
        ,IS_TEMPLATE
        ,SOURCE_TYPE)values (
        #{ID}
        ,#{USER_ID}
        ,#{REFERENCE_ID}
        ,#{SENDER_ID}
        ,#{MESSAGE_CATEGORY}
        ,#{MESSAGE_TYPE}
        ,#{MESSAGE_CONTENT}
        ,#{CREATION_DATE}
        ,#{IS_READ}
        ,#{OPEN_TYPE}

        ,#{IMPORTANT_LEVEL}
        ,#{LINK_TYPE}
        ,#{IS_AT}
        ,#{IS_TRACK}
        ,#{IS_REPLY}
        ,#{IS_TEMPLATE}
        ,#{SOURCE_TYPE}
        )
    </insert>
</mapper>